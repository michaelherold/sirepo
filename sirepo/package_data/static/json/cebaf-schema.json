{
    "appModes": {
        "default": {
            "localRoute": "beamline"
        }
    },
    "constants": {
        "configColumns": [
            {"name":  "Setting", "type": "label", "field": "setting"},
            {"name":  "Element", "type": "label", "field": "element"},
            {"name":  "Input/Output", "type": "label", "field": "io"},
            {"name":  "Model Tolerance", "type": "input", "field": "tolerance"},
            {"name":  "Channel Id", "type": "label", "field": "chanId"}
        ],
        "clight": 299792458,
        "particleMassAndCharge": {
            "positron": [0.510998928e-03, 1],
            "electron": [0.510998928e-03, -1],
            "proton": [0.938272046, 1],
            "antiproton": [0.938272046, -1],
            "posmuon": [0.1056583715, 1],
            "negmuon": [0.1056583715, -1],
            "ion": [0.931494061, 1]
        },
        "readoutElements": {
            "HKICKER": {
                "fields": ["current_kick"],
                "group": "Kickers [A]"
            },
            "KICKER": {
                "fields": ["current_hkick", "current_vkick"],
                "group": "Kickers [A]"
            },
            "QUADRUPOLE": {
                "fields": ["current_k1"],
                "group": "Quads [A]"
            },
            "VKICKER": {
                "fields": ["current_kick"],
                "group": "Kickers [A]"
            }
        }
    },
    "dynamicFiles": {
        "sirepoLibs": {
            "js": [
                "cebaf.js",
                "sirepo-lattice.js"
            ],
             "css": [
                 "lattice.css"
            ]
        }
    },
    "enum": {
        "ParticleType": [
            ["positron", "Positron"],
            ["electron", "Electron"],
            ["proton", "Proton"],
            ["antiproton", "Anti-proton"],
            ["posmuon", "Positive Muon"],
            ["negmuon", "Negative Muon"],
            ["ion", "Ion"],
            ["other", "Other"]
        ]
    },
    "localRoutes": {
        "beamline": {
            "route": "/beamline/:simulationId",
            "config": {
                "controller": "CEBAFBeamlineController as cebafBeamline",
                "templateUrl": "/static/html/cebaf-beamline.html"
            }
        },
        "config": {
            "route": "/config/:simulationId",
            "config": {
                "controller": "CEBAFConfigController as cebafConfig",
                "templateUrl": "/static/html/cebaf-config.html"
            }
        },
        "lattice": {
            "route": "/lattice/:simulationId"
        }
    },
    "model": {
