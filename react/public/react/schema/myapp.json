{
    "enum": {
        "Gender": [
            ["male", "Male"],
            ["female", "Female"]
        ],
        "DogDisposition": [
            ["aggressive", "Aggressive"],
            ["friendly", "Friendly"],
            ["submissive", "Submissive"]
        ]
    },
    "model": {
        "dog": {
            "breed": ["Breed", "String"],
            "gender": ["Gender", "Gender", "male"],
            "height": ["Height [cm]", "Float", 50.0, "Distance from front paws to withers"],
            "weight": ["Weight [lbs]", "Float", 60.5],
            "disposition": ["Disposition", "DogDisposition", "friendly"],
            "favoriteTreat": ["Favorite Treat", "OptionalString", ""]
        }
    },
    "view": {
        "dog": {
            "title": "Dog",
            "type": "panel",
            "config": {
                "basic": [
                    {
                        "layout": "tabs",
                        "tabs": [
                            {
                                "name": "Physical Characteristics",
                                "items": [
                                    {
                                        "layout": "fieldTable",
                                        "columns": ["Height", "Weight"],
                                        "rows": [
                                            {
                                                "label": "Size/Shape",
                                                "fields": [
                                                    "dog.height",
                                                    "dog.weight"
                                                ]
                                            }
                                        ]
                                    },
                                    {
                                        "layout": "fieldList",
                                        "fields": ["dog.breed"]
                                    }
                                ]
                            },
                            {
                                "name": "Personality",
                                "items": [
                                    {
                                        "layout": "fieldList",
                                        "fields": [
                                            "dog.disposition",
                                            "dog.favoriteTreat"
                                        ]
                                    }
                                ]
                            }
                        ]
                    }
                ],
                "advanced": [
                    {
                        "layout": "fieldList",
                        "fields": [
                            "dog.breed",
                            "dog.gender",
                            "dog.weight",
                            "dog.height"
                        ]
                    }
                ]
            }
        },
        "heightWeightReport": {
            "title": "Physical Characteristics",
            "type": "panel",
            "config": {
                "basic": [
                    {
                        "layout": "graph2d",
                        "report": "heightWeightReport"
                    }
                ]
            }
        }
    }
}
